            <link href="{{ asset('frontOffice/assets/css/remixicon.css') }}"  rel="stylesheet" />
            <link href="{{ asset('frontOffice/assets/css/meanmenu.min.css') }}"  rel="stylesheet" />
            <link href="{{ asset('frontOffice/assets/css/odometer.min.css') }}"  rel="stylesheet" />
            <link href="{{ asset('frontOffice/assets/css/animate.min.css') }}"  rel="stylesheet" />
            <link href="{{ asset('frontOffice/assets/css/style.css') }}"  rel="stylesheet" />
            <link href="{{ asset('frontOffice/assets/css/dark-mode.css') }}"  rel="stylesheet" />
            <link href="{{ asset('frontOffice/assets/css/responsive.css') }}"  rel="stylesheet" />
